{
    "rules": {
    "strict": 0,
        "indent": ["error", 4, {"SwitchCase": 1}],
        "semi": ["error", "always"],
        "space-before-function-paren": 0,
        "react/no-did-mount-set-state": 0,
        "react/no-danger": 0,
        "react/jsx-curly-spacing": [2, "never", {"allowMultiline": true}],
        "jsx-quotes": ["error", "prefer-double"],
        "react/display-name": 0,
        "react/prop-types": [2, {"ignore": ["dispatch", "params"]}]
},
    "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
],
    "plugins": [
    "react",
    "node"
],
    "env": {
    "node": true,
        "browser": true,
        "mocha": true
},
    "globals": {
    "gapi": true,
        "googletag": true
},
    "parserOptions": {
    "ecmaFeatures": {
        "jsx": true,
            "modules": true
    }
},
    "parser": "babel-eslint"
}
